									<Input id="nsal" placeholder="0.00" type="Number" width="50%"/>
									<Label text="iv.Deductions u/s 16(iva + ivb + iv6)" id="label_ded"/>
									<Input id="ded" placeholder="0.00" type="Number" width="50%"/>
									<Label text=" a. Standard Deduction u/s 16(ia)" id="label_stded"/>
									<Input id="stded" placeholder="0.00" type="Number" width="50%"/>
									<Label text=" b.Entertainment Allowance u/s 16(ii)" id="label_eallw"/>
									<Input id="eallw" placeholder="0.00" type="Number" width="50%"/>
									<Label text=" c.Professional Tax u/s 16(iii)" id="label_ptax"/>
									<Input id="ptax" placeholder="0.00" type="Number" width="50%"/>
									<Label text="v. Income chargeable under the Head 'Salaries'(iii-iv)" id="label_incc"/>
									<Input id="incc" placeholder="0.00" type="Number" width="50%"/>
								</f:content>
							</f:SimpleForm>
