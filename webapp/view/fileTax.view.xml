<mvc:View height="100%" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout" xmlns="sap.m" controllerName="com.app.tax.TaxApp-alpha.controller.fileTax" xmlns:html="http://www.w3.org/1999/xhtml">
	<NavContainer id="wizardNavContainer">
		<pages>
			<Page id="wizardContentPage" showHeader="false">
				<content>
					<Wizard id="CreateProductWizard" class="sapUiResponsivePadding--header sapUiResponsivePadding--content" complete="wizardCompletedHandler">
						<!--###############################################################################################-->
						<!--paste here-->
						<!--#############################################salary##################################################-->
						<WizardStep id="OptionalInfoStep" validated="true" activate="optionalStepActivation" title=" Salary/Pension">
							<f:SimpleForm editable="true" layout="ResponsiveGridLayout" columnsM="2" labelSpanM="6" emptySpanM="1" emptySpanL="1" columnsL="2"
								labelSpanL="6">
								<f:content>
									<core:Title text="1.Gross Salary"/>
									<Label text="a. Salary as per section 17(1)" id="label_sala"/>
									<Input id="sala" placeholder="0.00" type="Number"/>
									<Label text="b. Value of perquisites as per section 17(2)" id="label_salb"/>
									<Input id="salb" placeholder="0.00" type="Number"/>
									<Label text="c. Profits in lieu of salary as per section 17(3)" id="label_salc"/>
									<Input id="salc" placeholder="0.00" type="Number"/>
									<Label text="Gross Salary (1a+1b+1c)" id="label_name" class="bold"/>
									<Input id="gsal" placeholder="0.00" type="Number"/>
									<!--<Text class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop bold18" maxLines="2"-->
									<!--	text="ii.Less:Allowances to the extent exempt u/s 10(Ensure that it is included in salary income u/s 17(1)/17(2)/17(3) )"></Text>-->
									<core:Title text="2.Deductions"/>
									<Label text="a. Standard Deduction u/s 16(ia)" id="label_stded"/>
									<Input id="stded" placeholder="0.00" type="Number"/>
									<Label text="b.Entertainment Allowance u/s 16(ii)" id="label_eallw"/>
									<Input id="eallw" placeholder="0.00" type="Number"/>
									<Label text="c.Professional Tax u/s 16(iii)" id="label_ptax"/>
									<Input id="ptax" placeholder="0.00" type="Number"/>
									<!--<Label text="v. Income chargeable under the Head 'Salaries'(iii-iv)" id="label_incc"/>-->
									<!--<Input id="incc" placeholder="0.00" type="Number"/>-->
									<Label text="Deductions u/s 16(2a+2b+2c)" id="label_ded" class="bold"/>
									<Input id="ded" placeholder="0.00" type="Number"/>
								</f:content>
							</f:SimpleForm>
							<!--#########################################Table control#######################################################-->
							<Title class="sapUiMediumMarginTop"
								text="Allowances to the extent exempt u/s 10 (Ensure that it is included in salary income u/s 17(1)/17(2)/17(3) ) "/>
							<Table id="ins" items="{/Products}">
								<columns>
									<Column width="60px">
										<Text text="SL.NO"/>
									</Column>
									<Column width="40%">
										<Text text="Nature of Exempt Allowance"/>
									</Column>
									<Column width="30%">
										<Text text="Description"/>
									</Column>
									<Column width="15%">
										<Text text="Amount"/>
									</Column>
									<Column width="5%"></Column>
								</columns>
								<items>
									<ColumnListItem >
										<cells>
											<!--<Button icon="sap-icon://delete" press="deleteRow" type="Reject"/>-->
											<Input value="{slno}"/>
											<Input value="{nat}"/>
											<Input value="{desc}"/>
											<Input value="{amt}"/>
											<Button icon="sap-icon://delete" press="deleteRow" type="Reject"/>
										</cells>
									</ColumnListItem>
								</items>
							</Table>
							<OverflowToolbar>
								<Button icon="sap-icon://add" text="Row" press="addRow"/>
								<ToolbarSpacer/>
								<Label text="3.Total allowance" labelFor="tAllowance" class="bold">
									<layoutData>
										<OverflowToolbarLayoutData group="2"/>
									</layoutData>
								</Label>
								<Input width="15%" id="tAllowance" placeholder="0.00" type="Number" class="margRightTallowance">
									<layoutData>
										<OverflowToolbarLayoutData shrinkable="true" group="2" minWidth="150px"/>
									</layoutData>
								</Input>
							</OverflowToolbar>
							<f:SimpleForm editable="true" layout="ResponsiveGridLayout" columnsM="2" labelSpanM="6" emptySpanM="1" emptySpanL="1" columnsL="2"
								labelSpanL="6">
								<f:content>
									<core:Title text=""/>
									<Label text="4.Net Salary (1-3)" class="bold"/>
									<Input placeholder="0.00" type="Number"/>
									<core:Title text=""/>
									<Label text="5.Chargeable under Head Salaries (4-2)" class="bold">
										<layoutData>
											<l:GridData span="XL2 L6 M3 S4"/>
										</layoutData>
										<!-- sap.ui.core.LayoutData -->
									</Label>
								<Input placeholder="0.00" type="Number"/>
							</f:content>
						</f:SimpleForm>
					</WizardStep>
					<!--#############################################House Property##################################################-->
					<WizardStep id="housePropertyStep" validated="true" activate="housePropertyStep" title=" House Property">
						<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="3" labelSpanM="4" labelSpanS="12"
							adjustLabelSpan="false" emptySpanXL="0" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="1" columnsM="1"
							singleContainerFullSize="false">
							<f:content>
								<Label text="Type of House Property" id="label_htyp"/>
								<ComboBox id="htyp" width="50%">
									<core:Item key="1" text="Self Occupied"/>
									<core:Item key="2" text="Let Out"/>
									<core:Item key="3" text="Deemed Let Out"/>
								</ComboBox>
								<Label text="i.Gross rent recieved/recievable/letable value during the year" id="label_grent"/>
								<Input id="grent" placeholder="0.00" type="Number" width="50%"/>
								<Label text="ii.Tax paid to local authorities" id="label_lotax"/>
								<Input id="lotax" placeholder="0.00" type="Number" width="50%"/>
								<Label text="iii.Annual Value (i-ii)" id="label_aval"/>
								<Input id="aval" placeholder="0.00" type="Number" width="50%"/>
								<Label text="iv.30% of Annual Value" id="label_thaval"/>
								<Input id="thaval" placeholder="0.00" type="Number" width="50%"/>
								<Label text="v.Interest payable on borrowed capital" id="label_intp"/>
								<Input id="intp" placeholder="0.00" type="Number" width="50%"/>
								<Label text="vi. Arrears/Unrealized Rent recieved during the year Less 30%" id="label-arrent"/>
								<Input id="arrent" placeholder="0.00" type="Number" width="50%"/>
								<Label text="vii. Income chargeable under the head 'House Property' (iii-iv-v)+vi" id="label_inchp"/>
								<Input id="inchp" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Less: Deductions u/s 57(iia) (In case of family pension only" id="label_osded"/>
								<Input id="osded" placeholder="0.00" type="Number"/>
								<Label text="4.Gross Total Income(If loss, put the figure in negative) (1+2+3)" id="label_gtinco"/>
								<Input id="gtinco" placeholder="0.00" type="Number"/>
							</f:content>
						</f:SimpleForm>
					</WizardStep>
					<WizardStep id="deductionsStep" validated="false" activate="deductionsStep" title="Deductions">
						<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="3" labelSpanM="4" labelSpanS="12"
							adjustLabelSpan="false" emptySpanXL="0" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="1" columnsM="1"
							singleContainerFullSize="false">
							<f:content>
								<Label text="Total Income (4-6)" id="label_totinc"/>
								<Input id="totinc" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Tax Payable on Total Income" id="label_taxpay"/>
								<Input id="taxpay" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Rebate u/s 87A" id="label_rebate"/>
								<Input id="rebate" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Tax Payable After Rebate (8-9)" id="label_tpaftrbt"/>
								<Input id="tpaftrbt" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Health and Education Cess @4% on (10)" id="label_hcess"/>
								<Input id="hcess" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Total Tax and Cess (10+11)" id="label_ttaxcess"/>
								<Input id="ttaxcess" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Relief u/s 89(Please ensure to submit Form 10E to claim this relief)" id="label_relf"/>
								<Input id="relf" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Balance Tax after Relief(12-13)" id="label_baltax"/>
								<Input id="baltax" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Interest u/s 234 A" id="label_intrsta"/>
								<Input id="intrsta" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Interest u/s 234 B" id="label_intrstb"/>
								<Input id="intrstb" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Interest u/s 234 C" id="label_intrstc"/>
								<Input id="intrstc" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Fee u/s 234F" id="label_fee"/>
								<Input id="fee" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Total Interest, Fee Payable (15a + 15b + 15c + 15d)" id="label_totint"/>
								<Input id="totint" placeholder="0.00" type="Number" width="50%"/>
								<Label text="Total Tax, Fee and Interest" id="label_tottax"/>
								<Input id="tottax" placeholder="0.00" type="Number" width="50%"/>
							</f:content>
						</f:SimpleForm>
					</WizardStep>
				</Wizard>
			</content>
			<footer>
				<OverflowToolbar>
					<ToolbarSpacer/>
					<Button text="Cancel" press="handleWizardCancel"/>
				</OverflowToolbar>
			</footer>
		</Page>
	</pages>
</NavContainer>
</mvc:View>